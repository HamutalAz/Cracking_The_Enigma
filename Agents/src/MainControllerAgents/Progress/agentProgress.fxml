<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainControllerAgents.Progress.AgentProgressController">
  <columnConstraints>
    <ColumnConstraints hgrow="NEVER" maxWidth="10.0" minWidth="10.0" prefWidth="10.0" />
      <ColumnConstraints hgrow="ALWAYS" />
      <ColumnConstraints hgrow="SOMETIMES" />
    <ColumnConstraints hgrow="NEVER" maxWidth="10.0" minWidth="10.0" prefWidth="10.0" />
      <ColumnConstraints hgrow="NEVER" maxWidth="10.0" minWidth="10.0" prefWidth="10.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints maxHeight="10.0" minHeight="10.0" prefHeight="10.0" vgrow="NEVER" />
      <RowConstraints maxHeight="1.7976931348623157E308" vgrow="NEVER" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="10.0" minHeight="10.0" prefHeight="10.0" vgrow="NEVER" />
  </rowConstraints>
   <children>
      <ProgressBar fx:id="progressBar" maxWidth="1.7976931348623157E308" progress="0.0" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER" />
      <Label styleClass="staticLabels" text="Missions In Queue:" GridPane.columnIndex="1" GridPane.rowIndex="3">
         <padding>
            <Insets right="5.0" />
         </padding>
      </Label>
      <Label styleClass="staticLabels" text="Missions In Progress:" GridPane.columnIndex="1" GridPane.rowIndex="4">
         <padding>
            <Insets right="5.0" />
         </padding>
      </Label>
      <Label styleClass="staticLabels" text="Completed Missions:" GridPane.columnIndex="1" GridPane.rowIndex="5">
         <padding>
            <Insets right="5.0" />
         </padding>
      </Label>
      <Label styleClass="staticLabels" text="Candidates Found:" GridPane.columnIndex="1" GridPane.rowIndex="6">
         <padding>
            <Insets right="5.0" />
         </padding>
      </Label>
      <Label fx:id="missionsInQueuePH" styleClass="dynamicLabels" GridPane.columnIndex="2" GridPane.rowIndex="3" />
      <Label fx:id="missionsInProgressPH" styleClass="dynamicLabels" GridPane.columnIndex="2" GridPane.rowIndex="4" />
      <Label fx:id="completedMissionsPH" styleClass="dynamicLabels" GridPane.columnIndex="2" GridPane.rowIndex="5" />
      <Label fx:id="candidatesFoundPH" styleClass="dynamicLabels" GridPane.columnIndex="2" GridPane.rowIndex="6" />
      <Label styleClass="sectionTitle" text="Progress" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
   </children>
</GridPane>
