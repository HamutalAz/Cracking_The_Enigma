<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FirstPageAllies.ContestsTable.ContestsTableController">
   <center>
      <TableView fx:id="contestsTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="500.0">
         <columns>
            <TableColumn fx:id="battleFieldColumn" minWidth="-1.0" prefWidth="-1.0" styleClass="tableCol" text="Battle Field" />
            <TableColumn fx:id="uboatColumn" minWidth="-1.0" prefWidth="-1.0" styleClass="tableCol" text="UBoat" />
            <TableColumn fx:id="contestStatusColumn" minWidth="-1.0" prefWidth="-1.0" styleClass="tableCol" text="Contest Status" />
            <TableColumn fx:id="levelColumn" minWidth="-1.0" prefWidth="-1.0" styleClass="tableCol" text="Level" />
            <TableColumn fx:id="requiredTeamsColumn" prefWidth="75.0" styleClass="tableCol" text="Required Teams" />
            <TableColumn fx:id="choiceColumn" prefWidth="75.0" styleClass="tableCol" text="Choice" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </center>
   <top>
      <Label styleClass="sectionTitle" text="Contests" BorderPane.alignment="CENTER" />
   </top>
   <bottom>
      <Button fx:id="readyBtn" mnemonicParsing="false" onAction="#readyBtnClicked" text="Ready" BorderPane.alignment="CENTER_RIGHT">
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </BorderPane.margin>
      </Button>
   </bottom>
</BorderPane>
