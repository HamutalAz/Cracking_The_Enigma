<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FirstPageAllies.AgentsTable.AgentsTableController">
   <center>
      <TableView fx:id="teamsTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="300.0">
         <columns>
            <TableColumn fx:id="nameColumn" minWidth="-1.0" prefWidth="-1.0" styleClass="tableCol" text="Name" />
            <TableColumn fx:id="threadsNumberColumn" minWidth="-1.0" prefWidth="-1.0" styleClass="tableCol" text="Threads Number" />
            <TableColumn fx:id="taskSizeColumn" minWidth="-1.0" prefWidth="-1.0" styleClass="tableCol" text="Task Size" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </center>
   <top>
      <Label styleClass="sectionTitle" text="Agents" BorderPane.alignment="CENTER" />
   </top>
</BorderPane>
